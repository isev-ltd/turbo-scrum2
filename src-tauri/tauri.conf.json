{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Turbo Scrum 2",
    "version": "0.0.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "all": true,
        "readFile": true,
        "writeFile": true,
        "scope": ["$APPDATA/*"]
      },
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": true,
        "startDragging": true,
        "create": true
      },
      "dialog": {
        "confirm": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTools",
      "copyright": "isev",
      "deb": {
        "depends": []
